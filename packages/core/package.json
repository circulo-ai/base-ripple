{
  "name": "@base-ripple/core",
  "version": "1.0.3",
  "license": "MIT",
  "homepage": "https://base-ripple.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/circulo-ai/base-ripple"
  },
  "keywords": [
    "ripple",
    "ripple-effect",
    "interaction",
    "feedback",
    "ui",
    "css",
    "dom",
    "accessibility",
    "typescript",
    "vanilla-js"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@base-ripple/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles.css": {
      "@base-ripple/source": "./src/styles.css",
      "default": "./dist/styles.css"
    }
  },
  "sideEffects": [
    "./dist/styles.css"
  ],
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "tags": [
      "scope:core"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
